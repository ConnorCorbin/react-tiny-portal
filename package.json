{
    "name": "@corbincon/react-tiny-portal",
    "license": "ISC",
    "version": "1.0.1",
    "description": "A tiny React portal component safe for client and server side rendering.",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "main": "./dist/index.umd.cjs",
    "module": "./dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.umd.cjs"
        }
    },
    "scripts": {
        "build": "vite build && tsc",
        "test": "echo \"Error: no test specified\" && exit 1",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ConnorCorbin/react-tiny-portal.git"
    },
    "keywords": [
        "react",
        "react-portal",
        "react-portal-component",
        "react-create-portal",
        "portal",
        "create-portal"
    ],
    "devDependencies": {
        "react": ">=16.0.0",
        "react-dom": ">=16.0.0",
        "@babel/core": "^7.20.7",
        "@storybook/addon-actions": "^6.5.15",
        "@storybook/addon-essentials": "^6.5.15",
        "@storybook/addon-interactions": "^6.5.15",
        "@storybook/builder-webpack4": "^6.5.15",
        "@storybook/manager-webpack4": "^6.5.15",
        "@storybook/react": "^6.5.15",
        "@storybook/testing-library": "0.0.13",
        "@types/react": "^18.0.26",
        "@types/react-dom": "^18.0.10",
        "babel-loader": "^8.3.0",
        "prettier": "2.8.1",
        "typescript": "^4.9.4",
        "vite": "^4.0.4"
    },
    "peerDependencies": {
        "react": ">=16.0.0",
        "react-dom": ">=16.0.0"
    }
